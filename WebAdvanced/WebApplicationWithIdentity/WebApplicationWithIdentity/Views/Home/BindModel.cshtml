@using WebApplicationWithIdentity.Models;

@model Student

    <form asp-controller="Home" asp-action="BindModel" method="post">

        <div asp-validation-summary="All"></div>

        <input asp-for="YearToBind" />
        <input asp-for="Password" />
        <span asp-validation-for="Password"></span>
        <input asp-for="RepeatPassword" />
        <input asp-for="Email" />
        <input asp-for="Number" />
        <input asp-for="ProductionYear" />
        <input asp-for="Description" />

        <input type="submit" name="name" value="" />
    </form>


@section Scripts 
{
    <partial name="..\Shared\_ValidationScriptsPartial.cshtml" />
}
